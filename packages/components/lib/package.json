{
  "name": "@plume-ui-react/lib",
  "version": "0.0.9",
  "description": "A lightweight TypeScript React UI component library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darioegb/plume-ui-react.git"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "lib",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "plume-ui",
    "plume-ui-react"
  ],
  "author": "Dario E Gonzalez B <darioegb@gmail.com>",
  "dependencies": {
    "@plume-ui-react/button": "workspace:*",
    "@plume-ui-react/spinner": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint-config-custom": "workspace:*",
    "prettier-config": "workspace:*",
    "react": "^18.1.0",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.11.0",
    "react-dom": ">=16.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
